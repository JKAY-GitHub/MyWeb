@TECHREPORT{Kay2007_AOL,
  author = { Kay, Judy and  Niu, William},
  title = {Accretion-based ontology learning for context reasoning},
  year = {2007},
  number = {608},
  month = {January},
  institution = {The University of Sydney},
  keywords = {project:Personis},
  abstract = {Accretion-based ontology learner (acol) is a ontology learner
      within the sago framework. it accumulates evidence that contributes to
      each relationship between concepts picked up by the ontology learner. this
      means that, instead of having a fixed relationship between concepts, acol
      retains the evidence about the relationships and calculates, or resolves,
      the relationship according to a given context based on user, time, and/or
      location. this can be used to adapt information delivery to different
      contexts when there are multiple sources for (ontology) learning. this
      will be deployed in the myplace system, which is a context-aware system
      designed to provide personalised information delivery in the building of
      school of information technologies at the university of sydney.},
}
