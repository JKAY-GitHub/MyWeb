@ARTICLE{Martine2013IJCCLp455,
	volume = {8},
	author = {R. Martinez-Maldonado and Y. Dimitriadis and A. Martinez-Mones and J. Kay and 
		K. Yacef},
	journal = {International Journal on Computer-Supported Collaborative Learning },
	title = {Capturing and analyzing verbal and physical collaborative learning interactions 
		at an enriched interactive tabletop},
	number = {4},
	career_best = {RobertoMartinez, JudyKay, KalinaYacef},
	year = {2013},
	keywords = {project:DataMiningForTabletop, best:JudyKay, best:JudyKay-2015-Fekete, best:JudyKay-2015-Reimann},
	pages = {455-485},
  note = {

	Archival paper on the comprehensive set of approaches to automatically mining
        of learning data, to infer collaboration quality in small group learning.
	Funded by Smart Services CRC.
	Rank A.

        },
	  longnote = {
	Archival paper on the comprehensive set of approaches to automatically mining rich collections
	of learning data to make inferences about the quality of the collaboration in small groups
	doing concept mapping.
	This work is the first to demonstrate that it is possible to do accurate assessments of how
	well a group of learners is collaborating, based on a broad set of accepted measures of
	good collaboration.
	This was possible because we created the first interactive tabletop system that can
	collect rich data about the precise activities of each learner and their spoken utterances.
	We used careful video analysis of the collaboration to establish the gold standard ground truth.
	The work was based on innovative approaches to machine learning and data mining to make effective
	use of this learning data to automatically determine collaboration quality.
	This work demonstrates the potential of emerging technologies to make rich inferences about learning.
	These can provide valuable foundations for learners to monitor their learning and reflect on it.
	For teachers, it provides new ways to gain insights into the learning progress,
	both to inform their teaching actions and to assess the effectiveness of their actions.
	Funded by DP0877665, Pervasive Lifelong User Modelling for User Controlled Personalisation and Augmented Cognition.
	Rank A.
	},

}
